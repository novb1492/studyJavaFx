<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.media.MediaView?>


<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" 
fx:controller="ex1.MainController">
   <children>
      <HBox prefHeight="300.0" prefWidth="352.0">
         <children>
            <MediaView fx:id="mediaView" fitHeight="300.0" fitWidth="350.0" />
         </children>
      </HBox>
      <Button fx:id="playButton" layoutX="383.0" layoutY="42.0" mnemonicParsing="false" onAction="#play" prefHeight="30.0" prefWidth="100.0" text="재생" />
      <Button fx:id="pauseButton" layoutX="383.0" layoutY="72.0" mnemonicParsing="false" onAction="#pause" prefHeight="30.0" prefWidth="100.0" text="멈춤" />
      <Button fx:id="stopButton" layoutX="383.0" layoutY="102.0" mnemonicParsing="false" onAction="#stop" prefHeight="30.0" prefWidth="100.0" text="중지" />
   </children>
</AnchorPane>
